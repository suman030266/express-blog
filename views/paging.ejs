<% if(totalPage > 1 ){ %>
	<div>
		<% if (!isFirstPage) { %>
			<span class="prepage"><a title="上一页" href="?p=<%= page-1 %>">上一页</a></span>
		<% } %>
		<p><%= page %></p>
		<% for(var i = 1; i <= totalPage; i++) { %>
			<% if(page == i){ %>
				<span class="on">
					<a title="第<%= i %>页" href="?p=<%= i %>"><%= i %></a>
				</span>
			<% }else{ %>
				<span>
					<a title="第<%= i %>页" href="?p=<%= i %>"><%= i %></a>
				</span>
			<% } %>
		<% } %>
		<% if(totalPage > 10 ){ %>
			<span>
				.....
			</span>
		<% } %>
		<span>总共<%= totalItems %>条</span>
		<span>总共<%= totalPage %>页</span>
		<% if (!isLastPage) { %>
			<span class="nextpage"><a title="下一页" href="?p=<%= page+1 %>">下一页</a></span>
		<% } %>
	</div>
<% } %>